-- Function to run all scripts
local function runAllScripts()
    for i, scriptFunc in ipairs(scripts) do
        pcall(function()
            scriptFunc()
        end)
    end
end

game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(character)
    localPlayerModel = character
    humanoidRootPart = character:WaitForChild("HumanoidRootPart")
end)


--------------------------------------------------------------------------------------------------------
local aimbotTab = library:addTab("Combat")
local visualTab = library:addTab("Visuals")
local miscTab = library:addTab("Misc")
local configTab = library:addTab("Settings")
local createconfigs = configTab:createGroup('left', 'Create Configs')
local configsettings = configTab:createGroup('left', 'Config Settings')
local uisettings = configTab:createGroup('center', 'UI Settings')
local othersettings = configTab:createGroup('right', 'Other')

local HitDick = aimbotTab:createGroup('left', 'Hitbox')

local BigEsp = visualTab:createGroup('left', 'Big Esp')

local FullDick = visualTab:createGroup('center', 'World')
local EspResources = visualTab:createGroup('right', 'Esp Resources')

local SpeedHack = miscTab:createGroup('left', 'SpeedHack')
local Spider = miscTab:createGroup('center', 'Spider')
local Spinner = miscTab:createGroup('left', 'Spinner')

local AutoReload = miscTab:createGroup('right', 'Weapon')

local brambus = false

AutoReload:addToggle({
    text = "Enable Weapon",
    flag = "weaponyes",
    callback = function(state)
        brambus = state
    end
})

-- Auto Reload
AutoReload:addToggle({
    text = "Auto Reload",
    flag = "Auto Reload",
    callback = function(state)
        local isRunning = state
        if isRunning and brambus then
            if scriptLoop then
                scriptLoop:Disconnect()
            end
            scriptLoop = game:GetService("RunService").Heartbeat:Connect(function()
                runAllScripts()
                wait(0.1)
            end)
        else
            if scriptLoop then
                scriptLoop:Disconnect()
                scriptLoop = nil
            end
        end
    end
})

-- SpeedHack
SpeedHack:addToggle({
    text = "Cframe speedhack",
    flag = "cframe_speedhack",
    callback = function(state)
        tpwalking = state
    end
}):addKeybind({
    flag = "cframe_key",
    key = Enum.KeyCode.V
})

SpeedHack:addList({
    text = "Keybind Mode",
    flag = "wal",
    values = {"hold", "toggle","always"},
    value = "hold",
    callback = function(value)
        mode = value
    end
})

SpeedHack:addSlider({
    text = "Speedhack value",
    flag = "speedhack_value",
    min = 0,
    max = 2,
    value = 0,
    increment = 0.1,
    callback = function(value)
        Configuration.tpwalkspeed = value
    end
})

