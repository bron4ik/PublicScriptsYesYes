local Configuration = {
    Fullbright = false,
    gay = false,
    sped = 2,
    tpwalkspeed = 0,

    SpeedHack = false,
    SpeedValue = 14.5,
    Spider = false,
    SpiderSpeed = 10,
    HitboxSettings = {
        work = false,
        bibo = "Head",
        SizeValue = 1,
        TransparencyValue = 0.5,
        colar = Color3.new(1, 0, 0),
    }
}

local isRunning = false
local scriptLoop = nil

local tpwalking = false


local esp = false
local localPlayerModel = game:GetService("Players").LocalPlayer.Character
if not localPlayerModel then
    localPlayerModel = game:GetService("Players").LocalPlayer.CharacterAdded:Wait()
end
local plr = localPlayerModel

local function Hebe()
    local playersFolder = workspace:FindFirstChild("Players")
    if not playersFolder then return end
    
    for _, playerModel in pairs(playersFolder:GetChildren()) do
        if playerModel ~= localPlayerModel and playerModel:IsA("Model") then
            local root = playerModel:FindFirstChild("HumanoidRootPart")
            local head = playerModel:FindFirstChild("Head")
            local humanoid = playerModel:FindFirstChildOfClass("Humanoid")
            
            if root and head and humanoid and humanoid.Health > 0 and Configuration.HitboxSettings.work then
                if Configuration.HitboxSettings.bibo == "Body" then
                    root.Size = Vector3.new(Configuration.HitboxSettings.SizeValue, Configuration.HitboxSettings.SizeValue, Configuration.HitboxSettings.SizeValue)
                    root.Transparency = Configuration.HitboxSettings.TransparencyValue
                    root.BrickColor = BrickColor.new(Configuration.HitboxSettings.colar)
                    root.CanCollide = false
                elseif Configuration.HitboxSettings.bibo == "Head" then
                    head.Size = Vector3.new(Configuration.HitboxSettings.SizeValue, Configuration.HitboxSettings.SizeValue, Configuration.HitboxSettings.SizeValue)
                    head.Transparency = Configuration.HitboxSettings.TransparencyValue
                    head.BrickColor = BrickColor.new(Configuration.HitboxSettings.colar)
                    head.CanCollide = false
                    head.Massless = true
                end
            end
        end
    end
end



local Lighting = game:GetService("Lighting")

local function Hee()
    if Lighting and not Configuration.Fullbright then
        oldTime = Lighting.ClockTime
    end
    
    if Configuration.Fullbright then
        if Lighting then
            if Lighting.ClockTime ~=12 then
                Lighting.ClockTime = 12
            end
        end
    end
end

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Camera = workspace.CurrentCamera
local LocalPlayer = Players.LocalPlayer
ESP = ESP or {
    Enabled = false,
    ShowBoxes = false,
    ShowNames = false,
    ShowTracers = false,
    ShowHealth = false,
    Color = nil,
    HealthColorMode = "match",
    HealthColorFunction = function(healthPercent)
        if healthPercent > 0.7 then
            return Color3.fromRGB(0, 255, 0)
        elseif healthPercent > 0.3 then
            return Color3.fromRGB(255, 165, 0)
        else
            return Color3.fromRGB(255, 0, 0)
        end
    end,
    MaxDistance = 1000
}


local ESPObjects = {}

local function newDrawing(type, props)
    local obj = Drawing.new(type)
    for k, v in pairs(props) do
        pcall(function() obj[k] = v end)
    end
    return obj
end

local function createESPElements()
    return {
        Box = newDrawing("Square", {Visible = false, Thickness = 2, Filled = false, Color = Color3.new(1,1,1)}),
        Name = newDrawing("Text", {Visible = false, Center = true, Outline = true, Size = 16, Font = 2, Color = Color3.new(1,1,1)}),
        Tracer = newDrawing("Line", {Visible = false, Thickness = 1, Color = Color3.new(1,1,1)}),
        HealthBar = newDrawing("Line", {Visible = false, Thickness = 4, Color = Color3.new(0,1,0)}),
        HealthText = newDrawing("Text", {Visible = false, Center = true, Outline = true, Size = 14, Font = 2})
    }
end

local function getRainbowColor(t)
    local freq = 2
    return Color3.new(
        math.sin(freq * t) * 0.5 + 0.5,
        math.sin(freq * t + 2) * 0.5 + 0.5,
        math.sin(freq * t + 4) * 0.5 + 0.5
    )
end

local function getBoxScreenPoints(cframe, size)
    local half = size / 2
    local points = {}
    local visible = true
    
    for x = -1,1,2 do
        for y = -1,1,2 do
            for z = -1,1,2 do
                local corner = cframe * Vector3.new(half.X*x, half.Y*y, half.Z*z)
                local screenPos, onScreen = Camera:WorldToViewportPoint(corner)
                if not onScreen then visible = false end
                table.insert(points, Vector2.new(screenPos.X, screenPos.Y))
            end
        end
    end
    
    return points, visible
end

local function hideAll(data)
    if data then
        for _, obj in pairs(data) do
            if obj and obj.Visible ~= nil then
                pcall(function() obj.Visible = false end)
            end
        end
    end
end

local function cleanESPObjects(model)
    if ESPObjects[model] then
        for _, obj in pairs(ESPObjects[model]) do
            if obj then
                pcall(function() obj:Remove() end)
            end
        end
        ESPObjects[model] = nil
    end
end

local function updateESP()
    if not ESP.Enabled then
        for model in pairs(ESPObjects) do
            cleanESPObjects(model)
        end
        return
    end

    local now = tick()
    local baseColor = ESP.Color or getRainbowColor(now)
    local screenCenter = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
    
    local playersFolder = workspace:FindFirstChild("Players")
    if not playersFolder then return end

    for _, playerModel in pairs(playersFolder:GetChildren()) do
        if playerModel ~= LocalPlayer.Character and playerModel:IsA("Model") then
            local humanoid = playerModel:FindFirstChildOfClass("Humanoid")
            local rootPart = playerModel:FindFirstChild("HumanoidRootPart")
            
            if humanoid and humanoid.Health > 0 and rootPart then
                local distance = (rootPart.Position - Camera.CFrame.Position).Magnitude
                if distance > ESP.MaxDistance then
                    cleanESPObjects(playerModel)
                    continue
                end

                local success, cframe, size = pcall(function()
                    return playerModel:GetBoundingBox()
                end)
                
                if success and cframe and size then
                    local points, visible = getBoxScreenPoints(cframe, size)
                    if not visible then
                        cleanESPObjects(playerModel)
                        continue
                    end

                    local data = ESPObjects[playerModel] or createESPElements()
                    ESPObjects[playerModel] = data

                    local minX, minY, maxX, maxY = math.huge, math.huge, -math.huge, -math.huge
                    for _, pt in ipairs(points) do
                        minX = math.min(minX, pt.X)
                        minY = math.min(minY, pt.Y)
                        maxX = math.max(maxX, pt.X)
                        maxY = math.max(maxY, pt.Y)
                    end

                    local boxWidth, boxHeight = maxX - minX, maxY - minY
                    local slimWidth = boxWidth * 0.7
                    local slimX = minX + (boxWidth - slimWidth) / 2
                    local healthRatio = math.clamp(humanoid.Health / humanoid.MaxHealth, 0, 1)

                    pcall(function()
                        data.Box.Visible = ESP.ShowBoxes
                        if ESP.ShowBoxes then
                            data.Box.Position = Vector2.new(slimX, minY)
                            data.Box.Size = Vector2.new(slimWidth, boxHeight)
                            data.Box.Color = baseColor
                        end


                        data.Name.Visible = ESP.ShowNames
                        if ESP.ShowNames then
                            data.Name.Text = playerModel.Name
                            data.Name.Position = Vector2.new(slimX + slimWidth/2, minY - 20)
                            data.Name.Color = baseColor
                        end

                        data.Tracer.Visible = ESP.ShowTracers
                        if ESP.ShowTracers then
                            data.Tracer.From = screenCenter
                            data.Tracer.To = Vector2.new(slimX + slimWidth/2, maxY)
                            data.Tracer.Color = baseColor
                        end

                        data.HealthBar.Visible = ESP.ShowHealth
                        data.HealthText.Visible = ESP.ShowHealth
                        if ESP.ShowHealth then
                            local barHeight = boxHeight * healthRatio
                            
                            data.HealthBar.From = Vector2.new(slimX - 6, maxY)
                            data.HealthBar.To = Vector2.new(slimX - 6, maxY - barHeight)
                            
                            data.HealthText.Text = math.floor(humanoid.Health) .. "/" .. math.floor(humanoid.MaxHealth)
                            data.HealthText.Position = Vector2.new(slimX - 6, maxY - barHeight)
                            
                            if ESP.HealthColorMode == "custom" and ESP.HealthColorFunction then
                                local healthColor = ESP.HealthColorFunction(healthRatio)
                                data.HealthBar.Color = healthColor
                                data.HealthText.Color = healthColor
                            else
                                data.HealthBar.Color = baseColor
                                data.HealthText.Color = baseColor
                            end
                        end
                    end)
                end
            else
                cleanESPObjects(playerModel)
            end
        end
    end
end

local function setupPlayerRemovedHandler()
    local playersFolder = workspace:FindFirstChild("Players")
    if playersFolder then
        playersFolder.ChildRemoved:Connect(function(child)
            cleanESPObjects(child)
        end)
    end
end

setupPlayerRemovedHandler()


--[[local function getObjectPosition(obj)
    if obj:IsA("BasePart") then
        return obj.Position
    elseif obj:IsA("Model") and obj.PrimaryPart then
        return obj.PrimaryPart.Position
    elseif obj:IsA("Model") then
        return obj:GetPivot().Position
    else
        return nil
    end
end

function createBillboardGui(obj)
    if obj:FindFirstChild("fgb") then
        return obj.fgb, obj.fgb.NameLabel
    end

    local billboardGui = Instance.new("BillboardGui")
    billboardGui.Name = "fgb"
    billboardGui.AlwaysOnTop = true
    billboardGui.Size = UDim2.new(4, 0, 2, 0)
    billboardGui.StudsOffset = Vector3.new(0, 3, 0)
    billboardGui.Adornee = obj
    billboardGui.Parent = obj
    billboardGui.Enabled = true 
    local nameLabel = Instance.new("TextLabel")

    nameLabel.Name = "NameLabel"
    nameLabel.Text = obj.Name
    nameLabel.Font = Enum.Font.SourceSansBold
    nameLabel.TextSize = 18
    nameLabel.TextColor3 = Color3.new(1, 1, 1) 
    nameLabel.BackgroundTransparency = 1
    nameLabel.Size = UDim2.new(1, 0, 0.5, 0)
    nameLabel.Position = UDim2.new(0, 0, 0, 0) 
    nameLabel.Parent = billboardGui

    return billboardGui, nameLabel
end
local oreBillboards = {}
local function EEE()
    local folder = workspace:FindFirstChild("Players") or workspace
    local ores = {}
    for _, obj in pairs(folder:GetChildren()) do
        if obj:IsA("Model") and not obj:FindFirstChild("fgb") then
            table.insert(ores, obj)
        end
    end

    for _, ore in pairs(ores) do
        if getObjectPosition(ore) then
            local billboardGui, nameLabel = createBillboardGui(ore)
            oreBillboards[ore] = {
                gui = billboardGui,
                lab = nameLabel
            }
        end
    end
end




local function gay()
    local plr = game:GetService("Players").LocalPlayer.Character
    if not plr then
    local plr = game:GetService("Players").LocalPlayer.CharacterAdded:Wait()
    end
    if not plr or not plr:FindFirstChild("HumanoidRootPart") then
    end

    local playerPos = plr.HumanoidRootPart.Position
    for ore, data in pairs(oreBillboards) do
        if ore and ore.Parent then
            local orePos = getObjectPosition(ore)
            if orePos then
                if esp then
                    local distance = (playerPos - orePos).Magnitude
                    data.gui.Enabled = true 
                    data.lab.Text = string.format("%s [%d studs]", ore.Name,math.floor(distance))
                else
                    data.gui.Enabled = false
                end
            end
        else
            oreBillboards[ore] = nil
        end
    end
end--]]

local resourceESPSettings = {
    enabled = false,
    maxDistance = 500,
    
    Resources = {
        enabled = true,
        maxDistance = 300,
        ores = {
            ["Brimstone Ore"] = {
                enabled = true,
                color = Color3.fromRGB(255, 255, 0),
                maxDistance = 400
            },
            ["Stone Ore"] = {
                enabled = true,
                color = Color3.fromRGB(160, 160, 160),
                maxDistance = 300
            },
            ["Iron Ore"] = {
                enabled = true,
                color = Color3.fromRGB(255, 165, 50),
                maxDistance = 300
            }
        },
        defaultColor = Color3.fromRGB(255, 165, 50)
    },
    
    ClothPlants = {
        enabled = true,
        color = Color3.fromRGB(0, 255, 0),
        maxDistance = 200
    }
}

local resourceCache = {}
local localPlayer = plrs.LocalPlayer
local character
local humanoidRootPart

local function scanResources()
    local Resources = workspace:FindFirstChild("Resources")
    local ClothPlants = workspace:FindFirstChild("ClothPlants")
    
    if Resources then
        for _, obj in ipairs(Resources:GetChildren()) do
            if obj:IsA("Model") then
                local primaryPart = obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")
                if primaryPart and not resourceCache[obj] then
                    resourceCache[obj] = {
                        position = primaryPart.Position,
                        folderType = "Resources",
                        billboard = nil
                    }
                end
            end
        end
    end
    
    if ClothPlants then
        for _, obj in ipairs(ClothPlants:GetChildren()) do
            if obj:IsA("Model") then
                local primaryPart = obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")
                if primaryPart and not resourceCache[obj] then
                    resourceCache[obj] = {
                        position = primaryPart.Position,
                        folderType = "ClothPlants",
                        billboard = nil
                    }
                end
            end
        end
    end
end

local function updateResourceESP(obj, data, distance)
    local shouldShow = distance <= resourceESPSettings.maxDistance
    
    if data.folderType == "Resources" then
        local oreSettings = resourceESPSettings.Resources.ores[obj.Name]
        shouldShow = shouldShow and resourceESPSettings.Resources.enabled 
                   and oreSettings and oreSettings.enabled
                   and distance <= (oreSettings.maxDistance or resourceESPSettings.Resources.maxDistance)
    elseif data.folderType == "ClothPlants" then
        shouldShow = shouldShow and resourceESPSettings.ClothPlants.enabled
                   and distance <= resourceESPSettings.ClothPlants.maxDistance
    end
    if shouldShow and not data.billboard then
        local billboardGui = Instance.new("BillboardGui")
        billboardGui.Name = "ResourceESP"
        billboardGui.AlwaysOnTop = true
        billboardGui.Size = UDim2.new(4, 0, 2, 0)
        billboardGui.StudsOffset = Vector3.new(0, 3, 0)
        billboardGui.Adornee = obj
        billboardGui.Parent = obj
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Name = "NameLabel"
        nameLabel.Text = obj.Name
        nameLabel.Font = Enum.Font.SourceSansBold
        nameLabel.TextSize = 18
        nameLabel.BackgroundTransparency = 1
        nameLabel.Size = UDim2.new(1, 0, 0.5, 0)
        nameLabel.Position = UDim2.new(0, 0, 0, 0)
        
        if data.folderType == "Resources" then
            local oreSettings = resourceESPSettings.Resources.ores[obj.Name]
            nameLabel.TextColor3 = oreSettings and oreSettings.color or resourceESPSettings.Resources.defaultColor
        else
            nameLabel.TextColor3 = resourceESPSettings.ClothPlants.color
        end
        
        nameLabel.Parent = billboardGui
        
        data.billboard = {
            gui = billboardGui,
            label = nameLabel
        }
    end
    
    if data.billboard then
        data.billboard.gui.Enabled = shouldShow
        if shouldShow then
            data.billboard.label.Text = string.format("%s [%d]", obj.Name, math.floor(distance))
        end
    end
end

local function updateResourceESPAll()
    if not resourceESPSettings.enabled then return end
    if not character or not humanoidRootPart then
        character = localPlayer.Character
        if character then
            humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        end
        if not humanoidRootPart then return end
    end
    
    local playerPos = humanoidRootPart.Position
    
    scanResources()
    
    for obj, data in pairs(resourceCache) do
        if obj.Parent then 
            local distance = (playerPos - data.position).Magnitude
            updateResourceESP(obj, data, distance)
        else 
            if data.billboard then
                data.billboard.gui:Destroy()
            end
            resourceCache[obj] = nil
        end
    end
end

scanResources()

local updateInterval = 0.5
local accumulator = 0
RunService.Heartbeat:Connect(function(dt)
    accumulator = accumulator + dt
    if accumulator >= updateInterval then
        accumulator = 0
        updateResourceESPAll()
    end
end)

local oldsped = 12

local function savee()
    if not Configuration.SpeedHack then
        if localPlayerModel and localPlayerModel:FindFirstChild('Humanoid') then
            local hum = localPlayerModel:FindFirstChild('Humanoid')
            if hum then
                oldsped = localPlayerModel.Humanoid.WalkSpeed
            end
        end
    end
end



local function speedii()
    --[[if not Configuration.SpeedHack then
        if localPlayerModel and localPlayerModel:FindFirstChild('Humanoid') then
            local hum = localPlayerModel:FindFirstChild('Humanoid')
            if hum then
                if oldsped > 10 and oldsped < Configuration.SpeedValue then
                    localPlayerModel.Humanoid.WalkSpeed = oldsped
                end
            end
        end
    end--]]
    if Configuration.SpeedHack then
        if localPlayerModel and localPlayerModel:FindFirstChild('Humanoid') then
            local hum = localPlayerModel:FindFirstChild('Humanoid')
            if hum.MoveDirection.Magnitude > 0 then
                localPlayerModel.Humanoid.WalkSpeed = Configuration.SpeedValue
            else
                if oldsped > 10 then
                    localPlayerModel.Humanoid.WalkSpeed = oldsped
                end
            end
        end
    end

end--]]

local mode1 = "hold"
local abb = false
local function spiderrr()
    local character = localPlayerModel
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    local humanoidRootPart = character.HumanoidRootPart

    spiderKeyActive = dildo("spider_key",mode1)
    
    if not spiderKeyActive and abb then
    library:notify("Spider Deactivated",0.5)
    abb = false
    end


    if Configuration.Spider and spiderKeyActive then
        if not abb then
            library:notify("Spider Activated",0.5)
            abb = true
        end
        local rayOrigin = humanoidRootPart.Position
        local rayDirection = humanoidRootPart.CFrame.LookVector * 2
        local raycastParams = RaycastParams.new()
        raycastParams.FilterDescendantsInstances = {character}
        raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
        local raycastResult = workspace:Raycast(rayOrigin, rayDirection, raycastParams)

        if raycastResult then
            -- Если есть стена впереди, поднимаемся
            local moveDirection = Vector3.new(0, 1, 0)
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                humanoidRootPart.Velocity = moveDirection * Configuration.SpiderSpeed
            elseif UserInputService:IsKeyDown(Enum.KeyCode.S) then
                humanoidRootPart.Velocity = moveDirection * -Configuration.SpiderSpeed
            else
                humanoidRootPart.Velocity = Vector3.new(0, 0, 0)
            end
        else
            humanoidRootPart.Velocity = Vector3.new(humanoidRootPart.Velocity.X, humanoidRootPart.Velocity.Y, humanoidRootPart.Velocity.Z)
        end
    end
end

local ab = false
local mode2 = "toggle"
local spinAngle = 0
local player = game.Players.LocalPlayer

local OriginalRotationState = true
-- Функция для обхода блокировки вращения в первом лице
local function updateHWH()
    local fgh = dildo("spinner_key",mode2)

    if not fgh and ab then
    library:notify("Spinbot Deactivated",0.5)
    ab = false
    end

    if Configuration.gay and fgh then
        if not ab then
        library:notify("Spinbot Activated",0.5)
        ab = true
        end
        local Camera = workspace.CurrentCamera
        if not Camera then return end
        local character = localPlayerModel
        if not character or not character:FindFirstChild("HumanoidRootPart") then return end
        local root = character.HumanoidRootPart
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if not humanoid then return end
        humanoid.AutoRotate = false
        local rotationAngle = -math.atan2(Camera.CFrame.LookVector.Z, Camera.CFrame.LookVector.X) + tick() * Configuration.sped % 360
        local newRotation = CFrame.new(root.Position) * CFrame.Angles(0, rotationAngle, 0)
        root.CFrame = newRotation
    else
        local character = localPlayerModel
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if not humanoid then return end
        humanoid.AutoRotate = OriginalRotationState
    end
end



local svastik = false
local cframeKeyActive = false

local mode = "hold"
local pressed
local function uuu()
    local chr = localPlayerModel
    local hum = chr and chr:FindFirstChildOfClass("Humanoid")
    
    -- Проверяем состояние кейбинда из нового хранилища
    cframeKeyActive = dildo("cframe_key",mode)

    if not cframeKeyActive then
        svastik = true
    end

    if svastik and tpwalking and cframeKeyActive and chr and hum and hum.Parent then
        library:notify("SpeedHack Activated",0.5)
        while tpwalking and cframeKeyActive do
            svastik = false
            local delta = RunService.Heartbeat:Wait()
        
            if hum.MoveDirection.Magnitude > 0 then
                local speed = Configuration.tpwalkspeed or 1
                chr:TranslateBy(hum.MoveDirection * speed * delta * 10)
            end
        end
    end
end



