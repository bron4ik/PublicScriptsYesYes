-- Full Dick
FullDick:addToggle({
    text = "fullBright Toggle",
    flag = "fullbright_toggle",
    callback = function(state)
        Configuration.Fullbright = state
        if not state then
            local Lighting = game:GetService("Lighting")
            if Lighting then
                Lighting.ClockTime = oldTime
            end
        end
    end
})

FullDick:addButton({
    text = "Fullbright",
    callback = function()
        Lighting.Brightness = 2
        Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
        Lighting.GlobalShadows = false 
    end
})

